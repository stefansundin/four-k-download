./configure --prefix="./output/install/" \
--enable-shared \
--enable-static \
--disable-doc \
--disable-ffmpeg \
--disable-ffplay \
--enable-ffprobe \
--disable-ffserver \
--enable-avcodec \
--enable-avformat \
--disable-avdevice \
--enable-swscale \
--disable-avfilter \
--disable-pthreads \
--disable-network \
--disable-everything \
--enable-memalign-hack \
--enable-decoder=aac \
--enable-decoder=aac_latm \
--enable-decoder=vorbis \
--enable-decoder=rawvideo \
--enable-decoder=h264 \
--enable-decoder=h263 \
--enable-decoder=vp8 \
--enable-protocol=file \
--enable-demuxer=flv \
--enable-demuxer=matroska \
--enable-demuxer=mp4 \
--enable-demuxer=mov \
--enable-muxer=avi \
--enable-muxer=matroska \
--enable-muxer=mp4 \
--enable-muxer=mov \
--enable-encoder=aac \
--enable-encoder=mpeg4 \
--disable-debug \
--enable-runtime-cpudetect \
--enable-version3